{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "veg",
        "non-veg"
      ]
    },
    "isPopular": {
      "type": "boolean",
      "default": false
    },
    "iconName": {
      "type": "string"
    },
    "menu_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu-category.menu-category"
    }
  }
}
