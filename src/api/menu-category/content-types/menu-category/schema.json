{
  "kind": "collectionType",
  "collectionName": "menu_categories",
  "info": {
    "singularName": "menu-category",
    "pluralName": "menu-categories",
    "displayName": "Menu Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "richtext"
    },
    "displayOrder": {
      "type": "integer"
    },
    "imgUrl": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
